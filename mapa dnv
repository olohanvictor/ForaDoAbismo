// --------------------------------------------------------------------------------
// --- CLASSE 2: GERENCIADOR DE NÍVEL (NOVA) ---
// Lógica para trocar de fase e reposicionar o jogador. 
//
// vou ver oque eu comento aqui pq essa clase e majoritariamente I.A é por bruno da pra entender o basico 
// --------------------------------------------------------------------------------
public class GerenciadorDeNivel
{
    public static void VerificarTrocaDeMapa()
    {
        char[][] mapaAtual = ArmazenadorDeMapa.todosOsMapas[ArmazenadorDeMapa.mapaAtualIndex];

        // Verifica o que está embaixo do jogador AGORA
        // Nota: Certifique-se que pY e pX estão dentro dos limites do array para evitar IndexOutOfRangeException
        // acho q vcs entederam mas se a posição do jogador de alguma forma vazar da matriz vai bugar 
        // prestar atenção no esquema de colisão da movimentação
        // acho q se alguem tiver trocado a onde de entra e saida no caso os parenteses pdoe dar tui mas até onde eu se tá certo
        char pisoAtual = mapaAtual[ArmazenadorDeMapa.pY][ArmazenadorDeMapa.pX];

        // Se pisou na saída ')' ou '(', vai para o próximo nível
        if (pisoAtual == ')' || pisoAtual == '(')
        {
            CarregarProximoNivel();
        }
    }

    private static void CarregarProximoNivel()
    {
        ArmazenadorDeMapa.mapaAtualIndex++;

        // Verifica se acabaram os mapas 
        // se não tiver entednido errado quando os mapas acabarem aqui a tela vai limpar e fechar de boa.
        if (ArmazenadorDeMapa.mapaAtualIndex >= ArmazenadorDeMapa.todosOsMapas.Length)
        {
            Console.Clear();
            Environment.Exit(0);
        }

        // Procura onde o jogador deve nascer no novo mapa
        EncontrarEntradaDoJogador();
    }

    public static void EncontrarEntradaDoJogador()
    {
        char[][] mapa = ArmazenadorDeMapa.todosOsMapas[ArmazenadorDeMapa.mapaAtualIndex];

        for (int y = 0; y < mapa.Length; y++)
        {
            for (int x = 0; x < mapa[y].Length; x++)
            {
                if (mapa[y][x] == 'P')
                {
                    ArmazenadorDeMapa.pX = x;
                    ArmazenadorDeMapa.pY = y;
                    return; 
                }
            }
        }
    }
}

// --------------------------------------------------------------------------------
// --- CLASSE 3: LÓGICA DE IMPRESSÃO ---
// Utiliza CoresDoMapa para desenhar cada caractere.
// --------------------------------------------------------------------------------
public class ImprimiMapa 
{
    // metodo bem simples puxa as matrizes de ArmazenadorDeMapa de acordo com indice e vai imprimindo
    public static void DesenharMapa() 
    {
        // Acessa as variáveis estáticas da classe ArmazenadorDeMapa
        char[][] mapaAtual = ArmazenadorDeMapa.todosOsMapas[ArmazenadorDeMapa.mapaAtualIndex];

        // Salva a cor atual do console para restaurar no final
        ConsoleColor corOriginal = Console.ForegroundColor;

        try
        {
            Console.SetCursorPosition(0, 0);
        }
        catch
        {
            // Fallback caso o console não suporte cursor (ex: output simples)
            Console.Clear();
        }

        // Título que indica qual mapa está sendo desenhado 
        Console.WriteLine($"========= Mapa C# (Nível {ArmazenadorDeMapa.mapaAtualIndex + 1}) =========");

        for (int i = 0; i < mapaAtual.Length; i++)
        {
            // Itera caractere por caractere para aplicar cores individuais
            // vai pegando os caracteres e seguindo padrao de cor estabelecido pela classe cores do mapa
            for (int j = 0; j < mapaAtual[i].Length; j++)
            {
                char caractere = mapaAtual[i][j];

                // Define a cor de primeiro plano
                Console.ForegroundColor = CoresDoMapa.ObterCorDoCaractere(caractere);

                // Imprime o caractere
                Console.Write(caractere);
            }

            // Pula para a próxima linha após o término da linha do mapa
            Console.WriteLine();
        }

        // Restaura a cor original do console
        Console.ForegroundColor = corOriginal;
    }
}

// --------------------------------------------------------------------------------
// --- CLASSE 4: LÓGICA DE CORES ---
// Define a cor para cada tipo de caractere no mapa.
// --------------------------------------------------------------------------------
public static class CoresDoMapa
{
    // Retorna a cor de primeiro plano (Foreground) para um dado caractere do mapa 
    // vai pegando cada caracter e estipulando uma cor pra ele
    // por isso ficou meio dificl de fazer as "paredes de cores" diferentes por serem o mesmo caracter
    // se tiver solução é a gente não sabe fomos buchinhas
    public static ConsoleColor ObterCorDoCaractere(char c)
    {
        switch (c)
        {
            // ELEMENTOS COMUNS
            case '[':
            case ']':
            case '}':
            case '_':
                // Paredes e Portas/Barras: Cinza Padrão (Gray)
                return ConsoleColor.Gray; 

            case 'P':
                // Jogador (Prisioneiro): Amarelo Escuro/DarkYellow
                return ConsoleColor.Yellow; 

            case 'C':
                // Carcereiro/Guarda: Azul Claro (Cyan) para contraste
                return ConsoleColor.Cyan; 

            case '*':
                // Perigo/Armadilha: Vermelho
                return ConsoleColor.Red; 

            case ')':
            case '(':
                // Saída/Entrada: Verde Brilhante (Green) para destaque
                return ConsoleColor.Green; 

            // ELEMENTOS DO MAPA LAGO
            case '♣':
                // Árvore: Verde
                return ConsoleColor.Green; 

            case '≈':
                // Água/Lago: Azul
                return ConsoleColor.Blue;

            case '|':
                // Borda de legenda
                return ConsoleColor.DarkGray; 

            default:
                // Resto (chão/espaço vazio): Verde Escuro (DarkGreen)
                return ConsoleColor.DarkGray;
        }
    }
}

